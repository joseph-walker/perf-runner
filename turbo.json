{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"env": ["BRAND"],
			"dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				"build/**",
				".next/**",
				"!.next/cache/**",
				"!build/cache/**"
			]
		},
		"dev": {
			"cache": false,
			"persistent": true
		},
		"format": {
			"cache": false
		},
		"lint": {
			"outputs":  []
		},
		"test": {
			"outputs": ["coverage/**", "coverage-report/**"]
		}
	}
}
